{
  "name": "afghan-translator-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "cross-env NEXT_PUBLIC_ENVIRONMENT=prod next build",
    "start": "npx serve@latest out",
    "start:local": "cross-env NEXT_PUBLIC_ENVIRONMENT=local next dev",
    "start:dev": "cross-env NEXT_PUBLIC_ENVIRONMENT=dev next dev",
    "start:prod": "npm run build:prod && cross-env NEXT_PUBLIC_ENVIRONMENT=prod npx serve@latest out",
    "lint": "next lint",
    "sync:ios": "next build && npx cap sync && npx cap open ios",
    "live": "npx cap run ios --livereload --external",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^6.0.0",
    "@capacitor/android": "^6.1.2",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/clipboard": "^6.0.0",
    "@capacitor/core": "^6.1.2",
    "@capacitor/dialog": "^6.0.0",
    "@capacitor/filesystem": "^6.0.1",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/storage": "^1.2.5",
    "@capawesome/capacitor-file-picker": "^6.0.1",
    "@ionic/pwa-elements": "^3.3.0",
    "capacitor-document-scanner": "^2.0.0",
    "cross-env": "^7.0.3",
    "detectlanguage": "^2.1.0",
    "ldrs": "^1.0.2",
    "mdocument-scanner": "^0.0.4",
    "moesif-browser-js": "^1.8.13",
    "next": "14.2.10",
    "piexifjs": "^1.0.6",
    "primereact": "^10.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.14.1",
    "sharp": "^0.32.6",
    "tesseract.js": "^5.1.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "cross-env": "^7.0.3"
  }
}
